BEGIN ~JA#MONGO~

IF ~RandomNum(4,1)~ THEN JA#MONGO_3
  SAY @2
  IF ~~ THEN EXIT
END

IF ~RandomNum(4,2)~ THEN JA#MONGO_4
  SAY @3
  IF ~~ THEN EXIT
END

IF ~RandomNum(4,3)~ THEN JA#MONGO_5
  SAY @3
  IF ~~ THEN EXIT
END

IF ~RandomNum(4,4)~ THEN JA#MONGO_6
  SAY @4
  IF ~~ THEN EXIT
END

// END JA#MONGO


CHAIN IF WEIGHT #-1 ~Global("JA#MONGO_ALORA","LOCALS",0) InParty("ALORA") InMyArea("ALORA") !StateCheck("ALORA",CD_STATE_NOTVALID)~ THEN JA#MONGO JA#MONGO_1
  @0 DO ~SetGlobal("JA#MONGO_ALORA","LOCALS",1)~
  == %ALORA_JOINED% @5 = @6
  == %EDWIN_JOINED% IF ~InParty("EDWIN") InMyArea("EDWIN") !StateCheck("EDWIN",CD_STATE_NOTVALID)~ THEN @7
  BRANCH ~InParty("EDWIN") InMyArea("EDWIN") !StateCheck("EDWIN",CD_STATE_NOTVALID) InParty("QUAYLE") InMyArea("QUAYLE") !StateCheck("QUAYLE",CD_STATE_NOTVALID)~ BEGIN
    == %QUAYLE_JOINED% @8
    == %EDWIN_JOINED% @9
  END
EXIT


CHAIN IF WEIGHT #-1 ~Global("JA#MONGO_CORAN","LOCALS",0) InParty("CORAN") InMyArea("CORAN") !StateCheck("CORAN",CD_STATE_NOTVALID)~ THEN JA#MONGO JA#MONGO_2
  @1 DO ~SetGlobal("JA#MONGO_CORAN","LOCALS",1)~
  == %CORAN_JOINED% @10
EXIT
